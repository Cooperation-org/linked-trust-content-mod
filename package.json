{
  "private": true,
  "name": "content-moderation",
  "version": "1.0.0",
  "description": "Content moderation with Human protocol",
  "repository": "https://github.com/Cooperation-org/linked-trust-content-mod",
  "license": "MIT",
  "scripts": {
    "dev:docker": "docker compose --env-file=.env.development up -d",
    "exchange": "cd exchange && yarn && yarn start",
    "launcher-client": "cd launcher/client && yarn && yarn start",
    "launcher-server": "cd launcher/server && yarn && yarn start",
    "recording-oracle": "cd recording-oracle && yarn && yarn dev",
    "reputation-oracle": "cd reputation-oracle && yarn && yarn dev",
    "launcher-service": "cd launcher-service && yarn && yarn migrate:dev && yarn dev",
    "dev": "concurrently --kill-others-on-fail \"yarn launcher-client\" \"yarn launcher-server\" \"yarn launcher-service\" \"yarn exchange\" \"yarn recording-oracle\" \"yarn reputation-oracle\" \"yarn dev:docker\"",
    "local": "concurrently --kill-others-on-fail \"yarn launcher-client\" \"yarn launcher-server\" \"yarn launcher-service\" \"yarn recording-oracle\" \"yarn reputation-oracle\" \"yarn exchange\""
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "@typescript-eslint/utils": "^5.42.1",
    "concurrently": "^7.5.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {}
}
